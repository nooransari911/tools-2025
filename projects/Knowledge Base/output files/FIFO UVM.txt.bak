// fifo_monitor.sv
import fifo_pkg::*;

class fifo_monitor #(parameter int DATA_WIDTH = 8);

  virtual fifo_intf #(DATA_WIDTH) vif;
  mailbox #(fifo_transaction #(DATA_WIDTH)) prod_mon2scb;
  mailbox #(fifo_transaction #(DATA_WIDTH)) cons_mon2scb;
  
  function new(
    virtual fifo_intf #(DATA_WIDTH) vif,
    mailbox #(fifo_transaction #(DATA_WIDTH)) prod_mon2scb,
    mailbox #(fifo_transaction #(DATA_WIDTH)) cons_mon2scb
  );
    this.vif = vif;
    this.prod_mon2scb = prod_mon2scb;
    this.cons_mon2scb = cons_mon2scb;
  endfunction

  task main();
    $display("MONITOR: Starting to monitor both producer and consumer ports.");
    fork
      monitor_producer();
      monitor_consumer();
    join_none
  endtask

  // Watches the producer (write) side of the interface
  task monitor_producer();
    forever begin
      // Manually wait for the clock edge. We are not using a clocking block.
      @(posedge vif.clk); 
      
      // Sample the raw interface signals as seen by the DUT.
      // This logic now correctly mirrors the DUT's 'do_write' condition.
      if (vif.p_producer_valid && vif.d_producer_ready && vif.p_write_en) begin
        fifo_transaction #(DATA_WIDTH) txn = new();
        txn.txn_id = vif.ip_id; // Capture the ID from the bus
        txn.data = vif.p_write_data;
        prod_mon2scb.put(txn);
        //$display("MONITOR (PROD): Saw write of txn #%0d", txn.txn_id);
      end
    end
  endtask
  
  // Watches the consumer (read) side of the interface
  task monitor_consumer();
    forever begin
      // Manually wait for the clock edge.
      @(posedge vif.clk);

      // Sample the raw interface signals as seen by the DUT.
      // This logic now correctly mirrors the DUT's 'do_read' condition.
      if (vif.d_consumer_valid && vif.c_consumer_ready && vif.c_read_en) begin
        fifo_transaction #(DATA_WIDTH) txn = new();
        // The read_data and op_id are outputs from the DUT's flops,
        // so they are stable after the clock edge where 'do_read' was true.
        // We need to sample them carefully. The 'do_read' is asserted for one cycle.
        // The DUT will place data on the bus in the *next* cycle.
        
        // Correct read sampling: Data is valid on the cycle AFTER the read enable.
        // Therefore, we wait one more cycle to sample the output.
        @(posedge vif.clk);

        txn.actual_data = vif.data.read_data;
        txn.actual_op_id = vif.op_id;
        cons_mon2scb.put(txn);
        //$display("MONITOR (CONS): Saw read of txn with id #%0d", txn.actual_op_id);
      end
    end
  endtask

endclass

