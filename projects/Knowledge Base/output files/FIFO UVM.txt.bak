// fifo_transaction.sv
class fifo_transaction #(parameter int DATA_WIDTH = 8);

  protected static int next_id = 0;
  int unsigned txn_id;

  // Stimulus
  rand logic [DATA_WIDTH-1:0] data;

  // Fields to be populated by the testbench components
  logic [DATA_WIDTH-1:0] actual_data;
  logic [7:0]            actual_op_id;    // UPDATED: To capture the output ID from the FIFO
  logic [DATA_WIDTH-1:0] expected_data;
  logic [7:0]            expected_op_id;  // UPDATED: To hold the expected ID for comparison

  function new();
    this.txn_id = next_id++;
  endfunction

  function void print_stimulus(string tag = "STIM");
    $display("\n\n[%s] TXN #%0d: Sent Data = 0x%h", tag, txn_id, data);
  endfunction
  
  function void print_full(string result);
    $display("\n\n-------------------- TXN #%0d | %s --------------------", txn_id, result);
     $display("  Stimulus: Data=0x%h, ID=%0d", data, txn_id);
     $display("  Expected: Data=0x%h, ID=%0d", expected_data, expected_op_id);
     $display("  Actual:   Data=0x%h, ID=%0d", actual_data, actual_op_id);
     $display("------------------------------------------------------------------");
  endfunction

endclass

