package fifo_pkg;

  class fifo_transaction #(parameter int DATA_WIDTH = 8);

    protected static int next_id = 0;
    int unsigned txn_id;

    // Stimulus (driven by driver)
    rand logic [DATA_WIDTH-1:0] data;
    
    // Observed Response (captured by monitor)
    logic [DATA_WIDTH-1:0] actual_data;
    logic [7:0]            actual_op_id;

    function new();
      this.txn_id = next_id++;
    endfunction
    
    function void print_full(string result);
      $display("\n\n-------------------- TXN #%0d | %s --------------------", txn_id, result);
      $display("  Stimulus: Data=0x%h, ID=%0d", data, txn_id);
      $display("  Expected: Data=0x%h, ID=%0d", data, txn_id); // Expected is same as stimulus
      $display("  Actual:   Data=0x%h, ID=%0d", actual_data, actual_op_id);
      $display("------------------------------------------------------------------");
    endfunction

  endclass

endpackage

