  mailbox #(fifo_transaction #(DATA_WIDTH)) gen2drv;

  function new(mailbox #(fifo_transaction #(DATA_WIDTH)) gen2drv);
    this.gen2drv = gen2drv;
  endfunction

  task main(int num_txns);
    repeat (num_txns) begin
      fifo_transaction #(DATA_WIDTH) txn = new();
      if (!txn.randomize()) begin
        $fatal(0, "Transaction randomization failed!");
      end
      
      gen2drv.put(txn);
    end
    $display("GENERATOR: Finished sending all %0d transactions.", num_txns);
    done = 1'b1;
  endtask

endclass

