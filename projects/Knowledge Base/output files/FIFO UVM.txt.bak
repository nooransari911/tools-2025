// design.sv
module sync_fifo #(
  parameter DEPTH = 16,
  parameter ALMOST_FULL_THRESH = 12,
  parameter ALMOST_EMPTY_THRESH = 4
)(
  fifo_intf.fifo_core fifo_bus
);
  localparam DATA_WIDTH = $bits (fifo_bus.data.write_data);
  localparam PTR_WIDTH  = $clog2(DEPTH);

  typedef struct packed {
    logic [DATA_WIDTH-1:0] data;
    logic [7:0]            id;
  } fifo_entry_t;


  logic [PTR_WIDTH-1:0]   write_ptr, read_ptr;
  fifo_entry_t memory [DEPTH];
  logic [PTR_WIDTH-1:0] fill_level;

  logic [PTR_WIDTH:0]     next_write_ptr, next_read_ptr;
  fifo_entry_t next_write_entry;
  logic [7:0]			  next_ip_id, next_op_id;

  logic do_write, do_read;



  assign fill_level = write_ptr - read_ptr;
  assign fifo_bus.fill_count = fill_level;
  assign fifo_bus.status.empty = (fill_level == 0);
  assign fifo_bus.status.full  = (fill_level == DEPTH);
  assign fifo_bus.status.almost_empty = (fill_level <= ALMOST_EMPTY_THRESH);
  assign fifo_bus.status.almost_full  = (fill_level >= ALMOST_FULL_THRESH);

  assign fifo_bus.d_producer_ready = !fifo_bus.status.full;
  assign fifo_bus.d_consumer_valid = !fifo_bus.status.empty;

  assign do_write = fifo_bus.control.write_en && fifo_bus.d_producer_ready && fifo_bus.p_producer_valid;
  assign do_read  = fifo_bus.control.read_en  && fifo_bus.d_consumer_valid && fifo_bus.c_consumer_ready;
  assign next_write_entry.data = fifo_bus.data.write_data;
  assign next_write_entry.id = fifo_bus.ip_id;



  always_comb begin
    next_write_ptr = write_ptr;
    next_read_ptr  = read_ptr;
    next_op_id	   = '0;


    if (do_write) begin
      next_write_ptr = write_ptr + 1;
    end

    if (do_read) begin
      next_read_ptr = read_ptr + 1;
    end
  end

  always_ff @(posedge fifo_bus.clk) begin
    write_ptr <= next_write_ptr;
    read_ptr  <= next_read_ptr;


    if (!fifo_bus.rst_n) begin
      write_ptr <= '0;
      read_ptr  <= '0;
      fifo_bus.data.read_data <= '0;
      fifo_bus.op_id <= '0;
    end else begin
      if (do_write) begin
        memory[write_ptr] <= next_write_entry;
      end

      if (do_read) begin
        fifo_bus.data.read_data <= memory [read_ptr].data;
        fifo_bus.op_id <= memory [read_ptr].id;
      end
    end
  end

endmodule

