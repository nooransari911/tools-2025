// fifo_consumer.sv
import fifo_pkg::*;

class fifo_consumer #(parameter int DATA_WIDTH = 8);

  virtual fifo_intf #(DATA_WIDTH) .consumer vif;

  function new(virtual fifo_intf #(DATA_WIDTH) .consumer vif);
    this.vif = vif;
  endfunction
  
  task main();
    wait(vif.rst_n);
    $display("CONSUMER (Lazy): Reset deasserted, starting to read with high-utilization strategy.");

    // We are always ready to accept data from the DUT
    vif.tb_cb.c_consumer_ready <= 1'b1;
    vif.tb_cb.c_read_en        <= 1'b0;
    
    forever begin
      // Wait for the next clock edge to sample the DUT status
      @(vif.tb_cb);
      
      // Case 1: High-priority read. FIFO is almost full or full.
      // We must read aggressively to relieve backpressure.
      if (vif.tb_cb.status.almost_full || vif.tb_cb.status.full) begin
      //if (vif.tb_cb.status.full) begin
        if (vif.tb_cb.d_consumer_valid) begin
          //$display("CONSUMER (Lazy): High Priority Read at time %0t. Fill count: %0d", $time, vif.tb_cb.fill_count);
          perform_read();
        end
      end
      
      // Case 2: Low-priority read. Data is available, but no urgency.
      // Wait a random number of cycles to let the FIFO fill up.
      else if (vif.tb_cb.d_consumer_valid) begin
        int delay_cycles;
        delay_cycles = $urandom_range(1, 5); // Wait 1 to 5 cycles
        //$display("CONSUMER (Lazy): Low Priority - delaying read for %0d cycles at time %0t. Fill count: %0d", delay_cycles, $time, vif.tb_cb.fill_count);
        repeat(delay_cycles) @(vif.tb_cb);
        
        // After waiting, check again if data is still valid before reading
        if (vif.tb_cb.d_consumer_valid) begin
          perform_read();
        end
      end
      
      // Case 3: No data available. Do nothing, just wait for the next cycle.
      else begin
        // The forever loop's initial @(vif.tb_cb) handles the wait.
      end
    end
  endtask

  // Helper task for a single read operation
  task perform_read();
    vif.tb_cb.c_read_en <= 1'b1;
    @(vif.tb_cb);
    vif.tb_cb.c_read_en <= 1'b0;
  endtask

endclass

