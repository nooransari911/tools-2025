// performance_monitor.sv

class performance_monitor;

  virtual fifo_intf vif;
  
  // --- FIX: Changed from dynamic array to a queue ---
  int unsigned fill_count_history[$];
  
  function new(virtual fifo_intf vif);
    this.vif = vif;
  endfunction

  task main();
    $display("PERF_MONITOR: Starting to sample fill_count.");
    wait(vif.rst_n);
    forever begin
      @(posedge vif.clk);
      // Now this is the correct usage for a queue
      fill_count_history.push_back(vif.fill_count);
    end
  endtask

  // Function to print the collected data as a simple text-based graph
  function void print_report(int max_depth);
    int scale_factor;
    int bar_length;
    string bar;
    
    $display("\n-------------------- FIFO Fill Level vs. Time --------------------");
    $display("Max Depth: %0d | Samples Collected: %0d", max_depth, fill_count_history.size());
    $display("------------------------------------------------------------------");

    // To iterate over a queue, we can still use a standard for loop with index
    if (fill_count_history.size() == 0) return; // Prevent division by zero
    
    scale_factor = (fill_count_history.size() / 100) + 1; // Aim for max 100 lines
    
    for (int i = 0; i < fill_count_history.size(); i = i + scale_factor) begin
      bar = "";
      // Scale bar length to a reasonable width (e.g., 50 characters)
      bar_length = (fill_count_history[i] * 50) / max_depth;
      for (int j = 0; j < bar_length; j++) begin
        bar = {bar, "#"};
      end
      // Format the output line
      // Note: The time is i * 10ns because clk period is 10ns
      $display("Time %5dns | Fill: %2d | %s", i * 10, fill_count_history[i], bar);
    end
    
    $display("------------------------------------------------------------------\n");
  endfunction

endclass

