// fifo_driver.sv
import fifo_pkg::*;

class fifo_driver #(parameter int DATA_WIDTH = 8);
  
  virtual fifo_intf #(DATA_WIDTH) .driver vif;
  mailbox #(fifo_transaction #(DATA_WIDTH)) gen2drv;

  function new(
    virtual fifo_intf #(DATA_WIDTH) .driver vif,
    mailbox #(fifo_transaction #(DATA_WIDTH)) gen2drv
  );
    this.vif = vif;
    this.gen2drv = gen2drv;
  endfunction

  task main();
    fifo_transaction #(DATA_WIDTH) txn;
    wait(vif.rst_n);
    $display("DRIVER: Reset deasserted, starting to drive.");
    
    vif.tb_cb.p_write_en     <= 1'b0;
    vif.tb_cb.p_producer_valid <= 1'b0;
    vif.tb_cb.ip_id          <= '0;

    forever begin
      gen2drv.get(txn);
      wait(vif.tb_cb.d_producer_ready);
      
      @(vif.tb_cb);
      vif.tb_cb.p_write_en     <= 1'b1;
      vif.tb_cb.p_producer_valid <= 1'b1;
      vif.tb_cb.p_write_data   <= txn.data;
      vif.tb_cb.ip_id          <= txn.txn_id; // FIX: Drive the transaction ID
      
      @(vif.tb_cb);
      vif.tb_cb.p_write_en     <= 1'b0;
      vif.tb_cb.p_producer_valid <= 1'b0;
      vif.tb_cb.ip_id          <= '0;
    end
  endtask
endclass

