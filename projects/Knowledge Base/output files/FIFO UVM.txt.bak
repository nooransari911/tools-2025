// fifo_scoreboard.sv
import fifo_pkg::*;

class fifo_scoreboard #(parameter int DATA_WIDTH = 8);

  mailbox #(fifo_transaction #(DATA_WIDTH)) prod_mon2scb;
  mailbox #(fifo_transaction #(DATA_WIDTH)) cons_mon2scb;
  
  fifo_transaction #(DATA_WIDTH) expected_q[int]; // Associative array for in-flight txns
  
  int passed_txn_count = 0;
  int failed_txn_count = 0;
  
  // --- ADDITIONS for robust termination ---
  int   num_txns_to_check = 0;
  event all_txns_checked;
  // ----------------------------------------

  function new(
    mailbox #(fifo_transaction #(DATA_WIDTH)) prod_mon2scb,
    mailbox #(fifo_transaction #(DATA_WIDTH)) cons_mon2scb
  );
    this.prod_mon2scb = prod_mon2scb;
    this.cons_mon2scb = cons_mon2scb;
  endfunction

  task main();
    $display("SCOREBOARD: Starting...");
    fork
      collect_expected();
      check_actual();
    join_none
  endtask

  task collect_expected();
    fifo_transaction #(DATA_WIDTH) txn;
    forever begin
      prod_mon2scb.get(txn);
      expected_q[txn.txn_id] = txn;
    end
  endtask

  task check_actual();
    fifo_transaction #(DATA_WIDTH) actual_txn, expected_txn;
    forever begin
      cons_mon2scb.get(actual_txn);

      if (expected_q.exists(actual_txn.actual_op_id)) begin
        expected_txn = expected_q[actual_txn.actual_op_id];
        
        actual_txn.txn_id = expected_txn.txn_id;
        actual_txn.data   = expected_txn.data;

        if (actual_txn.actual_data === expected_txn.data) begin
          passed_txn_count++;
          actual_txn.print_full("PASSED");
        end else begin
          failed_txn_count++;
          actual_txn.print_full("FAILED - Data Mismatch");
          $error("SCOREBOARD: Mismatch on TXN #%0d", actual_txn.txn_id);
        end
        expected_q.delete(actual_txn.actual_op_id);
      end else begin
        failed_txn_count++;
        $error("SCOREBOARD: Received unexpected transaction with ID #%0d", actual_txn.actual_op_id);
      end
      
      // --- ADDITION: Check for test completion ---
      if ((passed_txn_count + failed_txn_count) == num_txns_to_check) begin
        -> all_txns_checked;
      end
      // ------------------------------------------
    end
  endtask

  task report_summary();
    if (expected_q.num() > 0) begin
      $error("SCOREBOARD: %0d transactions were sent but never received!", expected_q.num());
      foreach(expected_q[i]) begin
          $display("  --> Missing TXN #%0d", i);
          failed_txn_count++;
      end
    end

    $display("\n-------------------- TEST SUMMARY --------------------");
    $display("Total transactions checked: %0d", passed_txn_count + failed_txn_count);
    $display("Passed transactions: %0d", passed_txn_count);
    $display("Failed transactions: %0d", failed_txn_count);
    $display("------------------------------------------------------\n");
    if (failed_txn_count == 0 && (passed_txn_count == num_txns_to_check)) begin
      $display("********************* TEST PASSED *********************");
    end else begin
      $display("********************* TEST FAILED *********************");
    end
  endtask
endclass

