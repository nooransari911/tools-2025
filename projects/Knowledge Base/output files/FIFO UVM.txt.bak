// fifo_test.sv
import fifo_pkg::*;

class base_test #(parameter int DATA_WIDTH = 8);
  int unsigned num_txns_to_run = 100;
  fifo_environment #(DATA_WIDTH) env;

  function new();
  endfunction

  virtual task run_phase();
    env.scb.num_txns_to_check = num_txns_to_run;

    env.run_phase(num_txns_to_run);
    
    $display("TEST: Test components running. Waiting for scoreboard to signal completion...");
    wait(env.scb.all_txns_checked.triggered);
    
    $display("TEST: All transactions checked. Soaking for 50ns...");
    #50ns;

    // --- ADDITION: Print performance report before finishing ---
    env.perf_mon.print_report(16); // 16 is the DUT's DEPTH

    env.scb.report_summary();
    $finish;
  endtask

