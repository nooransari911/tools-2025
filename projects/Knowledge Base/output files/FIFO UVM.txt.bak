// fifo_intf.sv
interface fifo_intf #(
  parameter DATA_WIDTH = 8
);

  typedef struct packed {
    logic write_en;
    logic read_en;
  } fifo_control_t;

  typedef struct packed {
    logic full;
    logic empty;
    logic almost_full;
    logic almost_empty;
  } fifo_status_t;

  typedef struct packed {
    logic [DATA_WIDTH-1:0] write_data;
    logic [DATA_WIDTH-1:0] read_data;
  } fifo_data_t;

  typedef struct packed {
    logic ready;
    logic valid;
  } channel_handshake_t;


  logic clk;
  logic rst_n;
  logic [31:0] fill_count;
  logic [7:0] ip_id, op_id;


  logic p_write_en;
  logic c_read_en;
  logic [DATA_WIDTH-1:0] p_write_data;
  logic [DATA_WIDTH-1:0] c_read_data;
  logic p_producer_valid;
  logic d_producer_ready;
  logic d_consumer_valid;
  logic c_consumer_ready;



  fifo_control_t control;
  fifo_status_t  status;
  fifo_data_t    data;

  channel_handshake_t producer_hshake, consumer_hshake;


  assign control.write_en = p_write_en;
  assign control.read_en  = c_read_en;
  assign data.write_data  = p_write_data;
  assign c_read_data	  = data.read_data;
  assign producer_hshake.valid = p_producer_valid;
  assign producer_hshake.ready = d_producer_ready;
  assign consumer_hshake.valid = d_consumer_valid;
  assign consumer_hshake.ready = c_consumer_ready;

  clocking tb_cb @(posedge clk);
    default input #1step output #0ps;
    output p_write_en, c_read_en, p_write_data, ip_id, p_producer_valid, c_consumer_ready;
    input status, fill_count, c_read_data, op_id, d_producer_ready, d_consumer_valid;
  endclocking

  modport driver   (clocking tb_cb, input clk, input rst_n);
  modport consumer (clocking tb_cb, input clk, input rst_n);

  modport fifo_core (
    input  control,
    inout  data,
    output status,
    output fill_count,
    input  p_producer_valid,
    output d_producer_ready,
    output d_consumer_valid,
    input  c_consumer_ready,
    input  ip_id,
    output op_id,
    input  clk, 
    input  rst_n
  );
    
    
  modport monitor (
    input control,
    input status,
    input data,
    input fill_count,
    input producer_hshake,
    input consumer_hshake,
    input ip_id,
    input op_id,
    input clk, 
    input rst_n
  );

endinterface

